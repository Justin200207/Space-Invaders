public class Shield{
  int[][] y_pos = new int[4][4];
  int[][] x_pos = new int[4][4];
  int[][] health = new int[4][4];
  public Shield(int x, int y){
    for(int i = 0; i<4; i++){
      for(int o = 0; o<4; o++){
        x_pos[i][o] = x+i*20;
        y_pos[i][o] = y+i*20;
        health[i][o] = 3;
      }
    }
  }
  public int getX_pos(int i, int o){
    return x_pos[i][o];
  }
  public int getY_pos(int i,int o){
    return y_pos[i][o];
  }
}